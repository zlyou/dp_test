<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="276.0" prefWidth="423.0" stylesheets="@/style/style.css" xmlns="http://javafx.com/javafx/8.0.121"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.spawpaw.mybatis.generator.gui.controller.GenerationProgressController">
    <ProgressBar fx:id="progressbar" prefHeight="18.0" prefWidth="10000.0" progress="0.0" styleClass="primary">
        <VBox.margin>
            <Insets left="8.0" right="8.0" top="8.0"/>
        </VBox.margin>
    </ProgressBar>
    <TextArea fx:id="ta_msg" layoutX="7.0" layoutY="44.0" prefHeight="10000.0" prefWidth="10000.0">
        <VBox.margin>
            <Insets left="8.0" right="8.0" top="8.0"/>
        </VBox.margin>
    </TextArea>
    <HBox alignment="CENTER_RIGHT" prefHeight="47.0" prefWidth="10000.0">
        <Button fx:id="btn_ok" onMouseClicked="#onBtnOkClicked" prefHeight="23.0" prefWidth="58.0" text="OK"/>
        <VBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
        </VBox.margin>
    </HBox>
</VBox>
