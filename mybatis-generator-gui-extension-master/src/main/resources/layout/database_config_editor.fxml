<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="423.0" prefWidth="347.0" stylesheets="@/style/style.css" xmlns="http://javafx.com/javafx/8.0.121"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.spawpaw.mybatis.generator.gui.controller.DatabaseConfigEditorController">
    <VBox fx:id="main_container" prefHeight="1000.0">
        <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
        </padding>
    </VBox>
    <Label fx:id="lb_msg">
        <VBox.margin>
            <Insets left="8.0"/>
        </VBox.margin>
    </Label>
    <HBox alignment="CENTER_LEFT" minHeight="48.0">
        <Button minWidth="132.0" mnemonicParsing="false" onMouseClicked="#onTestConnectionClicked" prefWidth="120.0"
                text="%ui.testConnection"/>
        <HBox alignment="CENTER_LEFT" minWidth="80.0" prefHeight="48.0"/>
        <Button minWidth="72.0" mnemonicParsing="false" onMouseClicked="#onCancelClicked" prefWidth="60.0"
                text="%ui.cancel">
            <HBox.margin>
                <Insets right="8.0"/>
            </HBox.margin>
        </Button>
        <Button minWidth="72.0" mnemonicParsing="false" onMouseClicked="#onSaveClicked" prefWidth="60.0"
                styleClass="primary" text="%ui.save"/>
        <padding>
            <Insets left="8.0" right="8.0"/>
        </padding>
    </HBox>
</VBox>
